                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Change right_option key to left control on Internal Keyboard. (Post esc key when pressed alone)",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "identifiers": [{ "is_built_in_keyboard": true }],
                                        "type": "device_if"
                                    }
                                ],
                                "from": {
                                    "key_code": "right_option",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [{ "key_code": "left_control" }],
                                "to_if_alone": [{ "key_code": "escape" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
